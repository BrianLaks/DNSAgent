{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "DnsAgent": {
    "UpstreamDns": "8.8.8.8",
    "BlocklistUrl": "https://raw.githubusercontent.com/StevenBlack/hosts/master/hosts",
    "BlocklistUpdateSchedule": "0 2 * * *",
    "EnableWebUI": true,
    "WebUIPort": 5123,
    "EnableLogging": true,
    "LogRetentionDays": 5,
    "EnableBlocking": true,
    "UpstreamProtocol": "UDP",
    "DoHUrl": "https://dns.google/dns-query",
    "EnforceDnssec": false
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=dnsagent.db"
  },
  "AllowedHosts": "*"
}